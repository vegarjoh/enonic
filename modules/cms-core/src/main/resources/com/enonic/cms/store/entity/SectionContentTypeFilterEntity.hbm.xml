<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.enonic.cms.core.structure.menuitem.section" default-access="field">
  <class name="com.enonic.cms.core.structure.menuitem.section.SectionContentTypeFilterEntity" table="tseccontypefilter2" mutable="true">
    <cache usage="nonstrict-read-write"/>

    <id name="key" type="com.enonic.cms.store.hibernate.type.SectionContentTypeFilterKeyUserType">
      <column name="sctf_lkey"/>
      <generator class="com.enonic.cms.store.hibernate.id.IntegerBasedCustomIdentifierGenerator">
        <param name="table">tseccontypefilter2</param>
        <param name="idClassName">com.enonic.cms.core.structure.menuitem.section.SectionContentTypeFilterKey</param>
      </generator>
    </id>

    <many-to-one name="section" class="com.enonic.cms.core.structure.menuitem.MenuItemEntity" fetch="select">
      <column name="sctf_mei_lkey"/>
    </many-to-one>

    <many-to-one name="contentType" class="com.enonic.cms.core.content.contenttype.ContentTypeEntity" fetch="select">
      <column name="sctf_cty_lkey"/>
    </many-to-one>

  </class>


</hibernate-mapping>
